<script>
    $(document).ready(function () {
        var leftShow = false;
        var windowWidth = $(window).width();
        $('div.main').css('width', (windowWidth * 166 / 100) + 'px');
        $('div.menu').on('click', function () {
            if (leftShow) {
                window.scrollTo(3000, 0);
                leftShow = false;
            } else {
                window.scrollTo(0, 0);
                leftShow = true;
            }
        });
        window.scrollTo(3000, 0);
        $("#owl-example").owlCarousel({
            autoPlay: true,
            stopOnHover: true,
            singleItem: true
        });
        $('div.left').css('height', $('.right').height() + 'px');
        $('div.left').hide();
    });
</script>